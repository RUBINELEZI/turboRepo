{
  "name": "turbo-ts-tw",
  "private": true,
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "build": "turbo run build",
    "dev:web": "turbo run dev --filter=web --filter=api",
    "dev:admin": "turbo run dev --filter=admin --filter=api",
    "dev:business": "turbo run dev --filter=business --filter=api",
    "dev:screen": "turbo run dev --filter=screen --filter=api",
    "dev:emails": "turbo run dev --filter=emailer",
    "db:gen": "drizzle-kit generate:pg --config workspaces/services/drizzle/config.ts",
    "db:push": "ts-node ./shared/backend/drizzle/migrate",
    "db:seed": "ts-node ./shared/backend/drizzle/seed/",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@react-email/components": "0.0.6",
    "@react-email/render": "^0.0.7",
    "@tanstack/react-query": "^4.29.12",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.6.0",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "drizzle": "*",
    "drizzle-orm": "^0.26.5",
    "emailer": "*",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.241.0",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.3",
    "postgres": "^3.3.5",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "1.9.0",
    "react-icons": "^4.9.0",
    "react-leaflet": "^4.2.1",
    "react-simple-keyboard": "^3.6.7",
    "shad": "*",
    "sharp": "^0.32.1",
    "strooks": "*",
    "tailwind-config": "*",
    "tailwind-merge": "^1.13.1",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.18.1",
    "eslint": "^7.32.0",
    "eslint-config": "*",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.24",
    "prettier": "^2.5.1",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "turbo": "^1.10.2",
    "typescript": "^5.1.3"
  },
  "workspaces": [
    "workspaces/apps/*",
    "workspaces/services/*",
    "workspaces/configs/*",
    "workspaces/shared/**/*",
    "workspaces/emailer"
  ]
}
